<Silk>
<!--Prefixes-->
    <Prefixes>
      <Prefix id="rdf" namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
	  <Prefix id="owl" namespace="http://www.w3.org/2002/07/owl#"/>
	  <Prefix id="adb" namespace="http://rdf.geohistoricaldata.org/def/directory#"/>
	  <Prefix id="rdfs" namespace="http://www.w3.org/2000/01/rdf-schema#"/>
	  <Prefix id="locn" namespace="http://www.w3.org/ns/locn#"/>
	  <Prefix id="xsd" namespace="http://www.w3.org/2001/XMLSchema#"/>
	  <Prefix id="dcterms" namespace="http://purl.org/dc/terms/"/>
	  <Prefix id="rda" namespace="http://rdaregistry.info/Elements/a/"/>
    </Prefixes>
	<!--Datasets-->
	<DataSources>
	  <DataSource id="ebenistes1" type="sparqlEndpoint"><!--Nom du premier dataset à apparier-->
		  <Param name="pageSize" value="1000"></Param>
		  <Param name="pauseTime" value="0"></Param>
		  <Param name="retryCount" value="3"></Param>
		  <Param name="endpointURI" value="http://MacBook-Pro-di-admin.local:7200/repositories/ebenistesd2"></Param><!--Mettre l'adresse du répertoire GraphDB où se trouvent les données des annuaires-->
		  <Param name="graph" value="http://rdf.geohistoricaldata.org/id/directories/ebenistes_1845_1885"></Param><!--Mettre l'URI du graphe nommé où se trouvent les données sur lesquelles on travaille-->
		  <Param name="retryPause" value="1000"></Param>
		  <Param name="queryParameters" value=""/>
		  <Param name="login" value=""/>
		  <Param name="entityList" value=""/>
		  <Param name="parallel" value="true"/>
		  <Param name="password" value=""/>
	  </DataSource>
	  <DataSource id="ebenistes2" type="sparqlEndpoint"><!--Nom du second dataset à apparier-->
		  <Param name="pageSize" value="1000"></Param>
		  <Param name="pauseTime" value="0"></Param>
		  <Param name="retryCount" value="3"></Param>
		  <Param name="endpointURI" value="http://MacBook-Pro-di-admin.local:7200/repositories/ebenistesd2"></Param><!--Mettre l'adresse du répertoire GraphDB où se trouvent les données des annuaires-->
		  <Param name="graph" value="http://rdf.geohistoricaldata.org/id/directories/ebenistes_1845_1885"></Param><!--Mettre l'URI du graphe nommé où se trouvent les données sur lesquelles on travaille-->
		  <Param name="retryPause" value="1000"></Param>
		  <Param name="queryParameters" value=""/>
		  <Param name="login" value=""/>
		  <Param name="entityList" value=""/>
		  <Param name="parallel" value="true"/>
		  <Param name="password" value=""/>
	  </DataSource>
	</DataSources>
<!--Links-->
<Interlinks>
	<Interlink id="by-keys">
		<LinkType>owl:sameAs</LinkType>
			<SourceDataset dataSource="ebenistes1" var="a">
				<RestrictTo>{ ?a rdf:type adb:Entry } .</RestrictTo>
			</SourceDataset>
			<TargetDataset dataSource="ebenistes2" var="b">
				<RestrictTo>{ ?b rdf:type adb:Entry } .</RestrictTo>
			</TargetDataset>
<LinkageRule>
<!--Key 2 : label and activity-->
<!--
Conserve tous les cas où le score minimal de la combinaison de propriété Label-Activité est supérieur à 0
-->
<Aggregate id="min_label_activity" required="true" weight="1" type="min">
<!--
Pour deux entrées : calcule la distance character levenshtein entre les valeurs de la propriété Activité
-->
	<Compare id="levenshtein_activity" required="true" weight="1" metric="levenshtein" threshold="0.95">
<!--Distance maximale = 0.95-->
<!--
Transformation des valeurs : suppression des caractères spéciaux, des blancs en début et fin et passage en minuscules)
-->
		<TransformInput id="regexReplace_4" function="regexReplace">
				<TransformInput id="regexReplace_3" function="regexReplace">
					<TransformInput id="regexReplace_2" function="regexReplace">
						<TransformInput id="regexReplace_1" function="regexReplace">
							<TransformInput id="transfo_activity_source_1_lower" function="lowerCase">
								<Input id="source_activity_1" path="?a/rda:P50104"/>
								</TransformInput>
									<Param name="regex" value="[\[,.\-_\(\)\]?!':;$]"/>
									<Param name="replace" value=""/>
							</TransformInput>
							<Param name="regex" value="[éêëè]"/>
							<Param name="replace" value="e"/>
						</TransformInput>
				<Param name="regex" value="[àäâ]"/>
				<Param name="replace" value="a"/>
				</TransformInput>
			<Param name="regex" value="[ç]"/>
			<Param name="replace" value="c"/>
		</TransformInput>
		<!--
		Transformation des valeurs : suppression des caractères spéciaux, des blancs en début et fin et passage en minuscules)
		-->
		<TransformInput id="regexReplace_8" function="regexReplace">
			<TransformInput id="regexReplace_7" function="regexReplace">
					<TransformInput id="regexReplace_5" function="regexReplace">
						<TransformInput id="regexReplace_6" function="regexReplace">
							<TransformInput id="transfo_activity_target_2_lower" function="lowerCase">
								<Input id="target_activity_1" path="?b/rda:P50104"/>
								</TransformInput>
									<Param name="regex" value="[\[,.\-_\(\)\]?!':;$]"/>
									<Param name="replace" value=""/>
							</TransformInput>
							<Param name="regex" value="[éêëè]"/>
							<Param name="replace" value="e"/>
						</TransformInput>
				<Param name="regex" value="[àäâ]"/>
				<Param name="replace" value="a"/>
			</TransformInput>
			<Param name="regex" value="[ç]"/>
			<Param name="replace" value="c"/>
		</TransformInput>
	</Compare>
	<!--
	Pour deux entrées : calcule la distance d'édition Tokenwise entre les valeurs de la propriété Label
	-->
	<Compare id="levenshtein_persons" required="true" weight="1" metric="levenshtein" threshold="0.6">
	<!--Distance maximale = 0.6-->
		<!--
		Transformation des valeurs : suppression des caractères spéciaux, des blancs en début et fin et passage en minuscules)
		-->
		<TransformInput id="regexReplace_15" function="regexReplace">
			<TransformInput id="regexReplace_14" function="regexReplace">
					<TransformInput id="regexReplace_12" function="regexReplace">
						<TransformInput id="regexReplace_13" function="regexReplace">
							<TransformInput id="transfo_label_source_2_lower" function="lowerCase">
								<Input id="source_label_2" path="?a/rdfs:label"/>
								</TransformInput>
									<Param name="regex" value="[\[,.\-_\(\)\]?!':;$]"/>
									<Param name="replace" value=""/>
							</TransformInput>
							<Param name="regex" value="[éêëè]"/>
							<Param name="replace" value="e"/>
						</TransformInput>
				<Param name="regex" value="[àäâ]"/>
				<Param name="replace" value="a"/>
			</TransformInput>
			<Param name="regex" value="[ç]"/>
			<Param name="replace" value="c"/>
		</TransformInput>
		<!--
		Transformation des valeurs : suppression des caractères spéciaux)
		-->
		<TransformInput id="regexReplace_19" function="regexReplace">
			<TransformInput id="regexReplace_18" function="regexReplace">
					<TransformInput id="regexReplace_17" function="regexReplace">
						<TransformInput id="regexReplace_16" function="regexReplace">
							<TransformInput id="transfo_label_target_2_lower" function="lowerCase">
								<Input id="target_label_2" path="?b/rdfs:label"/>
								</TransformInput>
									<Param name="regex" value="[\[,.\-_\(\)\]?!':;$]"/>
									<Param name="replace" value=""/>
							</TransformInput>
							<Param name="regex" value="[éêëè]"/>
							<Param name="replace" value="e"/>
					</TransformInput>
				<Param name="regex" value="[àäâ]"/>
				<Param name="replace" value="a"/>
			</TransformInput>
			<Param name="regex" value="[ç]"/>
			<Param name="replace" value="c"/>
		</TransformInput>
	</Compare>
</Aggregate>
</LinkageRule>
<Filter limit="50"/>
<Outputs>
	<Output type="file" minConfidence="0.95">
		<Param name="file" value="labelactivity_accepted_links.nt"/>
		<Param name="format" value="ntriples"/>
	</Output>
</Outputs>
</Interlink>
</Interlinks>
</Silk>
